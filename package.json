{
    "name": "dpio-application",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "ng": "ng",
        "prettier-fix": "node ./node_modules/prettier/bin-prettier.js --write \"**/*.{js,json,css,md,ts,html,component.html}\"",
        "start:dev": "ng serve --proxy-config proxy.conf.json",
        "start": "ng serve --aot --proxy-config proxy.conf.js",
        "build": "ng build --prod --progress --build-optimizer",
        "test": "ng test --watch=false",
        "test:ci": "npm run lint -s && ng test ---watch=false",
        "lint": "ng lint --type-check",
        "translations:extract": "ngx-translate-extract --input ./apps --output ./libs/translations/template.json --format=json --clean -sort --marker extract",
        "e2e": "ng e2e",
        "update": "ng update",
        "update:check": "ng update",
        "generate": "ng generate"
    },
    "dependencies": {
        "@angular/animations": "^8.2.14",
        "@angular/cdk": "^8.2.3",
        "@angular/common": "^8.2.14",
        "@angular/compiler": "^8.2.14",
        "@angular/core": "^8.2.14",
        "@angular/flex-layout": "^8.0.0-beta.27",
        "@angular/forms": "^8.2.14",
        "@angular/language-service": "^8.2.14",
        "@angular/material": "^8.2.3",
        "@angular/platform-browser": "^8.2.14",
        "@angular/platform-browser-dynamic": "^8.2.14",
        "@angular/router": "^8.2.14",
        "@angular/service-worker": "^8.2.14",
        "@auth0/angular-jwt": "^3.0.1",
        "@ngrx/effects": "^8.5.2",
        "@ngrx/router-store": "^8.5.2",
        "@ngrx/store": "^8.5.2",
        "@ngrx/store-devtools": "^8.5.2",
        "@ngx-translate/core": "^11.0.1",
        "@ngx-translate/http-loader": "^4.0.0",
        "document-register-element": "^1.14.3",
        "hammerjs": "^2.0.8",
        "intl": "^1.2.5",
        "lodash": "^4.17.15",
        "material-design-icons": "^3.0.1",
        "ngrx-store-freeze": "^0.2.4",
        "ngx-uploader": "^8.0.0",
        "rxjs": "^6.5.3",
        "rxjs-compat": "^6.5.3",
        "tslib": "^1.10.0",
        "web-animations-js": "^2.3.2",
        "zone.js": "^0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.803.20",
        "@angular/cli": "~8.3.20",
        "@angular/compiler-cli": "^8.2.14",
        "@biesbjerg/ngx-translate-extract": "^4.2.0",
        "@ngrx/schematics": "^8.5.2",
        "@types/jasmine": "^3.5.0",
        "@types/jasminewd2": "~2.0.8",
        "@types/lodash": "^4.14.149",
        "@types/node": "^12.12.18",
        "codelyzer": "^5.2.0",
        "eslint": "^6.7.2",
        "https-proxy-agent": "^4.0.0",
        "jasmine-core": "^3.5.0",
        "jasmine-spec-reporter": "^4.2.1",
        "karma": "^4.4.1",
        "karma-chrome-launcher": "^3.1.0",
        "karma-cli": "^2.0.0",
        "karma-jasmine": "^2.0.1",
        "karma-jasmine-html-reporter": "^1.4.2",
        "karma-junit-reporter": "^2.0.1",
        "lint-staged": "9.5.0",
        "prettier": "^1.19.1",
        "protractor": "^5.4.2",
        "puppeteer": "^2.0.0",
        "ts-loader": "^6.2.1",
        "ts-node": "~8.5.4",
